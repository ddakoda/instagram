<html>
  <head>
      <title>instaMorning</title>
      <link rel="stylesheet" href="/style/main.css">
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/underscore.js/1.8.3/underscore-min.js"></script>
      <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/backbone.js/1.1.2/backbone-min.js"></script>
  </head>

  <body>

    <div class="container">
      <h1>Auth</h1>
       <h2>User Search</h2>
        <form action='/user_search'>
          <input type="text" name="username" placeholder="username">
          <input type="submit">
        </form>
    </div>

    <div class="container">
      <h1>Tags</h1>
       <h2>Search By Tags</h2>
        <form action='/tags'>
          <input type="text" name="tag" placeholder="tag words">
          <input type="submit">
        </form>
    </div>

    <div class="container">
      <h1>Tags</h1>
       <h2>Search By Tags</h2>
        <form action='/location_search'>
          <input type="text" name="lat" placeholder="latitude">
          <input type="text" name="long" placeholder="longitude">
          <input type="text" name="radius" placeholder="radius">
          <input type="submit">
        </form>
    </div>

  </body>
</html>
